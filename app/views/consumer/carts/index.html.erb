<p id="notice"><%= notice %></p>

<h1>Consumer Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumer_carts.each do |consumer_cart| %>
      <tr>
        <td><%= link_to 'Show', consumer_cart %></td>
        <td><%= link_to 'Edit', edit_consumer_cart_path(consumer_cart) %></td>
        <td><%= link_to 'Destroy', consumer_cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consumer Cart', new_consumer_cart_path %>
